#####################################################################################
# compute BEM model

export SUBJECTS_DIR=/neurospin/meg/meg_tmp/MTT_MEG_Baptiste/mri
export SUBJECT=rl130571

mne_init
mne_watershed_bem

cd ${SUBJECTS_DIR}/${SUBJECT}/bem
ln -s watershed/${SUBJECT}_inner_skull_surface inner_skull.surf
ln -s watershed/${SUBJECT}_outer_skin_surface outer_skin.surf
ln -s watershed/${SUBJECT}_outer_skull_surface outer_skull.surf

mkheadsurf -s $SUBJECT

mne_surf2bem --surf ${SUBJECTS_DIR}/${SUBJECT}/surf/lh.seghead --id 4 --check --fif ${SUBJECTS_DIR}/${SUBJECT}/bem/rl130571-head.fif

